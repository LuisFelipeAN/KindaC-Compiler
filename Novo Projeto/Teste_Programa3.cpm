int fatorial(int n){
 if(n < 1) {
 	return 1;
 }
 else{
	return n*fatorial(n-1);
  }
}

int main() {
	int a;
	int *b;
	b = &a;
	a=fatorial(6);
	*b = 1;
	readln(&a);
	print("F(6)=%d\nTeste\n%c", a, 'g');
}
